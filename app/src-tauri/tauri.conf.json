{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "ZSH Config Manager",
  "version": "0.3.0",
  "identifier": "com.nolyo.zsh-config-manager",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "ZSH Config Manager",
        "width": 1200,
        "height": 800,
        "maximized": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "active": true,
      "endpoints": [
        "https://api.github.com/repos/nolyo/zsh-config-manager/releases/latest"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDQ5MEM0MjQ4RTlCNkMwNjQKUldRTHhGWE5IV1FadnMxcHlKZExLaGxGQ0hGSGxHcVd3M09ZbE1QcHcvSlZJWjI4cGtZc3ZCNCtZVgphcHBsZWVkX3NpZ25hdHVyZQ=="
    }
  }
}